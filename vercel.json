{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "public"
        }
      },
      {
        "src": "src/html/*.html",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "public"
        }
      }
    ],
    "routes": [
      {
        "src": "src/html/*.html",
        "dest": "public"
      }
    ]
  }